{
    "name": "laravel-notification-channels/jabber-notification",
    "description": "laravel notification packages for jabber",
    "homepage": "https://github.com/laravel-notification-channels/jabber-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "minul islam",
            "email": "maanpatwary@gmail.com",
            "homepage": "http://demo.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "fabiang/xmpp": "^0.7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "^5.1|^5.2|^5.3|^5.4|^5.5|^5.6|^5.7",
        "illuminate/support": "^5.1|^5.2|^5.3|^5.4|^5.5|^5.6|^5.7"
    },
    "require-dev": {
           "nunomaduro/larastan": "^0.3.15",
        "phpunit/phpunit": "^7.0",
           "mockery/mockery": "^0.9.5",
        "orchestra/testbench": "3.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Jabber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Jabber\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Jabber\\JabberServiceProvider"
            ],
             "aliases": {
            "Jabber": "NotificationChannels\\Jabber\\Facade"
        }
        }
    }
}
