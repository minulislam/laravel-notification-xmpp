{
  "name": "f4bio/laravel-notification-xmpp",
  "description": "laravel notification packages for jabber",
  "homepage": "https://github.com/f4bio/laravel-notification-xmpp",
  "license": "MIT",
  "authors": [
    {
      "name": "minul islam",
      "email": "maanpatwary@gmail.com",
      "homepage": "http://demo.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.0|^8.0",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "illuminate/notifications": "^5.6|^6.0|^7.0|^8.0",
    "illuminate/queue": "^5.6|^6.0|^7.0|^8.0",
    "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
    "norgul/xmpp-php": "^2.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\Jabber\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\Jabber\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "NotificationChannels\\Jabber\\JabberServiceProvider"
      ]
    }
  }
}
